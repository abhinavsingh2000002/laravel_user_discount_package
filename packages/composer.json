{
  "name": "abhinav/laravel-user-discounts",
  "description": "A reusable Laravel package for user-level discounts with stacking, caps, audits.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Abhinav\\Discounts\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Abhinav\\Discounts\\DiscountsServiceProvider"
      ],
      "aliases": {
        "Discounts": "Abhinav\\Discounts\\Facades\\Discounts"
      }
    }
  },
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^11.0",
    "illuminate/database": "^11.0",
    "illuminate/events": "^11.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Abhinav\\Discounts\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^9.0"
  }
}
